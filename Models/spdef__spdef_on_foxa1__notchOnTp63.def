DESCRIPTION
"Model to describe RNAseq data for multiciliary tissue development."

PREDICTOR
t	T	"min"	"time"	0	1000

COMPARTMENTS
cyt	V	"pl"	"vol."	1
    
STATES
//number of cells of a specific type
multi_prog      C	"au"	"au"	cyt	1	"multipotent progenitor" 1
//multi_prog_mat  C	"au"	"au"	cyt	1	"multipotent progenitor with maternal Foxi1" 1
bsc1            C	"au"	"au"	cyt	1	"bsc1" 1
bsc2            C	"au"	"au"	cyt	1	"bsc2" 1
isc             C	"au"	"au"	cyt	1	"isc" 1
mcc             C	"au"	"au"	cyt	1	"mcc" 1
mcc_late        C	"au"	"au"	cyt	1	"mcc_late" 1
ssc             C	"au"	"au"	cyt	1	"ssc" 1

//ligand/NICD concentration in progenitor cell
ligand_rna      	C	"au"	"au"	cyt	1	"ligand_rna"   1
//ligand_rna_del1  	C	"au"	"au"	cyt	1	"ligand_rna_del1"   1
//ligand_rna_del2  	C	"au"	"au"	cyt	1	"ligand_rna_del2"   1
//ligand_rna_del3  	C	"au"	"au"	cyt	1	"ligand_rna_del3"   1
//ligand_rna_del  	C	"au"	"au"	cyt	1	"ligand_rna_del"   1
sum_ligand_rna	C	"au"	"au"	cyt	1	"sum_ligand_rna"   1

//hes* rna concentration in progenitor cell
hes4_rna    	C	"au"	"au"	cyt	1	"hes4_rna"       1
hes5_rna    	C	"au"	"au"	cyt	1	"hes5_rna"       1
hes7_rna    	C	"au"	"au"	cyt	1	"hes7_rna"       1
hes4_rna_del1   	C	"au"	"au"	cyt	1	"hes4_rna_del1"   1
hes5_rna_del1   	C	"au"	"au"	cyt	1	"hes5_rna_del1"   1
hes7_rna_del1   	C	"au"	"au"	cyt	1	"hes7_rna_del1"   1
hes4_rna_del2   	C	"au"	"au"	cyt	1	"hes4_rna_del2"   1
hes5_rna_del2   	C	"au"	"au"	cyt	1	"hes5_rna_del2"   1
hes7_rna_del2   	C	"au"	"au"	cyt	1	"hes7_rna_del2"   1
hes4_rna_del3   	C	"au"	"au"	cyt	1	"hes4_rna_del3"   1
hes5_rna_del3   	C	"au"	"au"	cyt	1	"hes5_rna_del3"   1
hes7_rna_del3   	C	"au"	"au"	cyt	1	"hes7_rna_del3"   1
hes4_rna_del   	C	"au"	"au"	cyt	1	"hes4_rna_del"   1
hes5_rna_del   	C	"au"	"au"	cyt	1	"hes5_rna_del"   1
hes7_rna_del   	C	"au"	"au"	cyt	1	"hes7_rna_del"   1

//tp63
tp63_rna    	C	"au"	"au"	cyt	1	"tp63_rna"   1
spdef_rna  	    C	"au"	"au"	cyt	1	"spdef_rna"  1
spdef_prot	    C	"au"	"au"	cyt	1	"spdef_prot"      1

INPUTS

REACTIONS
//cell production
//bsc1        -> multi_prog      CUSTOM "prod_multi_prog * hes7_rna_del * bsc1"	        "Multipotent progenitor production"
bsc1        -> multi_prog      CUSTOM "prod_multi_prog * bsc1"	        "Multipotent progenitor production"
            -> multi_prog      CUSTOM "prolif_multi_prog * (km_cell_off)^hill_cell_off/((km_cell_off)^hill_cell_off+tp63_rna^hill_cell_off)"               "MP proliferation"
            -> isc         	   CUSTOM "prod_isc        * multi_prog"	"Differentiation into ISC"
            -> mcc             CUSTOM "prod_mcc        * multi_prog"	"Differentiation into MCC"
            -> ssc             CUSTOM "prod_ssc        * multi_prog"	"Differentiation into SSC"
multi_prog  -> bsc2            CUSTOM "prod_bsc2       * multi_prog"	"Maturing of BSCs"

//MCCs stop expressing micdas
mcc         -> mcc_late        CUSTOM "prod_mcc_late * mcc"         	  "MCCs stop expressing micdas"

//ligand_rna turnover
ligand_rna      ->	        	        CUSTOM "deg_ligand_rna * ligand_rna"  "ligand_rna degradation"
      	        -> ligand_rna 	        CUSTOM "prod_ligand_rna"              "ligand_rna production"
//                -> ligand_rna_del1  	CUSTOM "ligand_rna/(hes_delay/4)"               "ligand_rna delay"
//ligand_rna_del1 -> ligand_rna_del2  	CUSTOM "ligand_rna_del1/(hes_delay/4)"               "ligand_rna delay"
//ligand_rna_del2 -> ligand_rna_del3  	CUSTOM "ligand_rna_del2/(hes_delay/4)"               "ligand_rna delay"
//ligand_rna_del3 -> ligand_rna_del   	CUSTOM "ligand_rna_del3/(hes_delay/4)"               "ligand_rna delay"
//ligand_rna_del  ->                  	CUSTOM "deg_ligand_rna_del * ligand_rna_del"         "ligand_rna delay"

	            ->	sum_ligand_rna 	CUSTOM "prod_sum_ligand_rna * ligand_rna"              "integrate ligand_rna"
sum_ligand_rna  ->	            	CUSTOM "forget_ligands * sum_ligand_rna"              "integrate ligand_rna"

//hes*_rna turnover
hes4_rna     ->       	    	CUSTOM "deg_hes4_rna * hes4_rna"          "hes4_rna degradation"
hes5_rna     ->       	    	CUSTOM "deg_hes5_rna * hes5_rna"          "hes5_rna degradation"
hes7_rna     ->       	    	CUSTOM "deg_hes7_rna * hes7_rna"          "hes7_rna degradation"
             ->	hes4_rna    	CUSTOM "prod_hes4_rna"      	          "hes4_rna production"
             ->	hes5_rna    	CUSTOM "prod_hes5_rna"      	          "hes5_rna production"
             ->	hes7_rna    	CUSTOM "prod_hes7_rna"      	          "hes7_rna production"
             -> hes4_rna_del1   	CUSTOM "hes4_rna/(hes_delay/4)"               "hes4_rna delay"
             -> hes5_rna_del1   	CUSTOM "hes5_rna/(hes_delay/4)"               "hes5_rna delay"
             -> hes7_rna_del1   	CUSTOM "hes7_rna/(hes_delay/4)"               "hes7_rna delay"
hes4_rna_del1 -> hes4_rna_del2   	CUSTOM "hes4_rna_del1/(hes_delay/4)"               "hes4_rna delay"
hes5_rna_del1 -> hes5_rna_del2   	CUSTOM "hes5_rna_del1/(hes_delay/4)"               "hes5_rna delay"
hes7_rna_del1 -> hes7_rna_del2   	CUSTOM "hes7_rna_del1/(hes_delay/4)"               "hes7_rna delay"
hes4_rna_del2 -> hes4_rna_del3   	CUSTOM "hes4_rna_del2/(hes_delay/4)"               "hes4_rna delay"
hes5_rna_del2 -> hes5_rna_del3   	CUSTOM "hes5_rna_del2/(hes_delay/4)"               "hes5_rna delay"
hes7_rna_del2 -> hes7_rna_del3   	CUSTOM "hes7_rna_del2/(hes_delay/4)"               "hes7_rna delay"
hes4_rna_del3 -> hes4_rna_del   	CUSTOM "hes4_rna_del3/(hes_delay/4)"               "hes4_rna delay"
hes5_rna_del3 -> hes5_rna_del   	CUSTOM "hes5_rna_del3/(hes_delay/4)"               "hes5_rna delay"
hes7_rna_del3 -> hes7_rna_del   	CUSTOM "hes7_rna_del3/(hes_delay/4)"               "hes7_rna delay"
hes4_rna_del ->             	CUSTOM "deg_hes4_rna_del * hes4_rna_del"  "hes4_rna delay"
hes5_rna_del ->             	CUSTOM "deg_hes5_rna_del * hes5_rna_del"  "hes5_rna delay"
hes7_rna_del ->             	CUSTOM "deg_hes7_rna_del * hes7_rna_del"  "hes7_rna delay"

tp63_rna   ->   	    	CUSTOM "deg_tp63_rna * tp63_rna"      "tp63_rna degradation"
           ->	tp63_rna   	CUSTOM "prod_tp63_rna"      	      "tp63_rna production"

spdef_rna     ->       	       CUSTOM "deg_spdef_rna * spdef_rna"          "spdef_rna degradation"
              -> spdef_rna     CUSTOM "prod_spdef_rna"        	           "spdef_rna production"
spdef_prot    ->       	       CUSTOM "deg_spdef_prot * spdef_prot"        "spdef_protein degradation"
              -> spdef_prot    CUSTOM "prod_spdef_prot * spdef_rna"        "spdef_protein production"

DERIVED
//cell populations
bsc                 C   "au"    "au"    "bsc1+bsc2"
mcc_total           C   "au"    "au"    "mcc+mcc_late"
n_cells             C   "au"    "au"    "bsc+multi_prog+isc+mcc_total+ssc"

//cell production rates
//hes_isc       C   "1/min"    "1/time"    "hes4_isc  * (hes4_rna_del) + hes5_isc  * (hes5_rna_del) + hes7_isc  * (hes7_rna_del)"
//hes_mcc       C   "1/min"    "1/time"    "hes4_mcc  * (hes4_rna_del) + hes5_mcc  * (hes5_rna_del) + hes7_mcc  * (hes7_rna_del)"
//hes_ssc       C   "1/min"    "1/time"    "hes4_ssc  * (hes4_rna_del) + hes5_ssc  * (hes5_rna_del) + hes7_ssc  * (hes7_rna_del)"

//hes_isc       C   "1/min"    "1/time"    "hes7_isc  * (hes7_rna_del)"
//hes_mcc       C   "1/min"    "1/time"    "hes4_mcc  * (hes4_rna_del)"
//hes_ssc       C   "1/min"    "1/time"    "hes5_ssc  * (hes5_rna_del)"

prod_isc      C   "1/min"    "1/time"    "v_max_isc * (hes7_rna_del)^hill_isc_on/((km_isc_on)^hill_isc_on+hes7_rna_del^hill_isc_on) * (km_cell_off)^hill_cell_off/((km_cell_off)^hill_cell_off+tp63_rna^hill_cell_off) * km_isc_off_hes4^hill_isc_off_hes4/(km_isc_off_hes4^hill_isc_off_hes4+(hes4_rna_del)^hill_isc_off_hes4) * km_isc_off_hes5^hill_isc_off_hes5/(km_isc_off_hes5^hill_isc_off_hes5+(hes5_rna_del)^hill_isc_off_hes5)"
prod_mcc      C   "1/min"    "1/time"    "v_max_mcc * (hes4_rna_del)^hill_mcc_on/((km_mcc_on)^hill_mcc_on+hes4_rna_del^hill_mcc_on) * (km_cell_off)^hill_cell_off/((km_cell_off)^hill_cell_off+tp63_rna^hill_cell_off) * km_mcc_off_hes5^hill_mcc_off_hes5/(km_mcc_off_hes5^hill_mcc_off_hes5+(hes5_rna_del)^hill_mcc_off_hes5)"
prod_ssc      C   "1/min"    "1/time"    "v_max_ssc * (hes5_rna_del)^hill_ssc_on/((km_ssc_on)^hill_ssc_on+hes5_rna_del^hill_ssc_on) * (km_cell_off)^hill_cell_off/((km_cell_off)^hill_cell_off+tp63_rna^hill_cell_off) * km_ssc_off_hes4^hill_ssc_off_hes4/(km_ssc_off_hes4^hill_ssc_off_hes4+(hes4_rna_del)^hill_ssc_off_hes4) * (spdef_prot)^hill_ssc_on_spdef/((km_ssc_on_spdef)^hill_ssc_on_spdef+spdef_prot^hill_ssc_on_spdef) * km_ssc_off_spdef^hill_ssc_off_spdef/(km_ssc_off_spdef^hill_ssc_off_spdef+(spdef_prot)^hill_ssc_off_spdef)"
//prod_ssc      C   "1/min"    "1/time"    "v_max_ssc * (hes_ssc)^hill_ssc_on/((km_ssc_on)^hill_ssc_on+hes_ssc^hill_ssc_on) * (km_cell_off)^hill_cell_off/((km_cell_off)^hill_cell_off+tp63_rna^hill_cell_off) * (tp63_rna)^hill_ssc_on/((km_ssc_on)^hill_ssc_on+tp63_rna^hill_ssc_on)"
prod_bsc2     C   "1/min"    "1/time"    "v_max_bsc2 * tp63_hill"

//ligand_rna production
prod_ligand_rna    C   "1/min"    "1/time"    "prod_ligand_rna_multi_prog*multi_prog/n_cells + prod_ligand_rna_bsc*bsc1/n_cells + prod_ligand_rna_bsc2*bsc2/n_cells + prod_ligand_rna_isc*isc/n_cells + prod_ligand_rna_mcc*mcc_total/n_cells + prod_ligand_rna_ssc*ssc/n_cells"

//hes* expression hill curves
prod_hes4_rna      C   "1/min"    "1/time"    "v_max_hes4 * (nicd_signal)^hill_hes4_on/(km_hes4_on^hill_hes4_on+(nicd_signal)^hill_hes4_on) * km_hes4_off^hill_hes4_off/(km_hes4_off^hill_hes4_off+(nicd_signal)^hill_hes4_off)"
prod_hes5_rna      C   "1/min"    "1/time"    "v_max_hes5 * (nicd_signal)^hill_hes5_on/(km_hes5_on^hill_hes5_on+(nicd_signal)^hill_hes5_on)"
prod_hes7_rna      C   "1/min"    "1/time"    "v_max_hes7 * km_hes7_off^hill_hes7_off/(km_hes7_off^hill_hes7_off+(nicd_signal)^hill_hes7_off) * km_hes7_off_spdef^hill_hes7_off_spdef/(km_hes7_off_spdef^hill_hes7_off_spdef+(spdef_prot)^hill_hes7_off_spdef)"

prod_spdef_rna     C   "1/min"    "1/time"    "v_max_spdef*(nicd_signal)^hill_spdef_on/((km_spdef_on)^hill_spdef_on+(nicd_signal)^hill_spdef_on)"

prod_tp63_rna      C   "1/min"    "1/time"    "v_max_tp63 * tp63_hill"
tp63_hill          C   "1/min"    "1/time"    "(nicd_signal)^hill_tp63_on/((km_tp63_on)^hill_tp63_on+(nicd_signal)^hill_tp63_on) + (spdef_prot)^hill_tp63_on_spdef/((km_tp63_on_spdef)^hill_tp63_on_spdef+(spdef_prot)^hill_tp63_on_spdef)"

nicd_signal        C   "1/min"    "1/time"    "(notch_lof_fc * sum_ligand_rna)+(notch_gof_fc-1)*init_sum_ligand_rna"

OBSERVABLES
        
ERRORS

CONDITIONS
//initial cell populations (only init_bsc1, init_multi_prog and init_isc not zero)
init_bsc2           "0"
init_mcc            "0"
init_mcc_late       "0"
init_ssc            "0"

//initial molecular concentrations:
//steady state for ligand_rna
init_ligand_rna       "(prod_ligand_rna_bsc*init_bsc1/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_multi_prog/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_isc_factor * prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_isc/(init_bsc1+init_multi_prog+init_isc))/deg_ligand_rna"
//init_ligand_rna_del1  "(prod_ligand_rna_bsc*init_bsc1/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_multi_prog/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_isc_factor * prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_isc/(init_bsc1+init_multi_prog+init_isc))/deg_ligand_rna"
//init_ligand_rna_del2  "(prod_ligand_rna_bsc*init_bsc1/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_multi_prog/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_isc_factor * prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_isc/(init_bsc1+init_multi_prog+init_isc))/deg_ligand_rna"
//init_ligand_rna_del3  "(prod_ligand_rna_bsc*init_bsc1/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_multi_prog/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_isc_factor * prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_isc/(init_bsc1+init_multi_prog+init_isc))/deg_ligand_rna"
//init_ligand_rna_del   "(((prod_ligand_rna_bsc*init_bsc1/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_multi_prog/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_isc_factor * prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_isc/(init_bsc1+init_multi_prog+init_isc))/deg_ligand_rna)/(hes_delay/4))/deg_ligand_rna_del"
init_sum_ligand_rna   "prod_sum_ligand_rna*((prod_ligand_rna_bsc*init_bsc1/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_multi_prog/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_isc_factor * prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_isc/(init_bsc1+init_multi_prog+init_isc))/deg_ligand_rna)/forget_ligands"
//init_tp63_rna         "(v_max_tp63*((((((prod_sum_ligand_rna*((prod_ligand_rna_bsc*init_bsc1/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_multi_prog/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_isc_factor * prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_isc/(init_bsc1+init_multi_prog+init_isc))/deg_ligand_rna)/forget_ligands))*notch_lof_fc)*notch_gof_fc)))^hill_tp63_on/((((prod_sum_ligand_rna*((prod_ligand_rna_bsc*init_bsc1/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_multi_prog/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_isc_factor * prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_isc/(init_bsc1+init_multi_prog+init_isc))/deg_ligand_rna)/forget_ligands)) + km_tp63_on_delta)^hill_tp63_on+((((((prod_sum_ligand_rna*((prod_ligand_rna_bsc*init_bsc1/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_multi_prog/(init_bsc1+init_multi_prog+init_isc) + (prod_ligand_rna_isc_factor * prod_ligand_rna_multi_prog_factor * prod_ligand_rna_bsc)*init_isc/(init_bsc1+init_multi_prog+init_isc))/deg_ligand_rna)/forget_ligands))*notch_lof_fc)*notch_gof_fc)))^hill_tp63_on))/deg_tp63_rna"
init_spdef_prot       "prod_spdef_prot * ((init_spdef * init_spdef_gof_fc)/scale_spdef)/deg_spdef_prot"

//steady state for hes*_rna:
